@model StudentSystem.Models.Student

<div class="Form">
    <h1>Register </h1>
    <form  id="">

        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.Label("Student Name", new { @class = "", @id = "Sname" })<br />
        @Html.TextBoxFor(Model => Model.stuName, new { @class = "name form-control", })
        @Html.ValidationMessageFor(model => model.stuName, "", new { @class = "text-danger" })
        <br />
       
        @Html.Label("Email")<br />
        @Html.TextBoxFor(Model => Model.email, new { @class = "name form-control" })
        @Html.ValidationMessageFor(model => model.email, "", new { @class = "text-danger" })
        <br />
        @Html.Label("Password")<br />
        @Html.PasswordFor(Model => Model.pwd, new { @class = "name form-control", @id = "pwd" })
        @Html.ValidationMessageFor(model => model.pwd, "", new { @class = "text-danger" })

                                        <br />
        @Html.Label("Confirm Password")<br />
        @Html.Password("Confirm", "", new { @class = "name form-control", @id = "cpwd" })

        <br />
        @Html.Label("Mobile Number")<br />
        @Html.TextBoxFor(Model => Model.mobNo, new { @class = "name form-control" })
        @Html.ValidationMessageFor(model => model.mobNo, "", new { @class = "text-danger" })
        <br />
        @Html.Label("Address")<br />
        @Html.TextBoxFor(Model => Model.Address, new { @class = "name form-control" })
        @Html.ValidationMessageFor(model => model.Address, "",new {@class = "text-danger" })
        <br />




        <button id="butt" class="btn btn-success " type="submit" formmethod="post" >Submit</button>
    </form>
    <script src="~/Content/Validation.js" type="text/javascript"></script>
</div>
